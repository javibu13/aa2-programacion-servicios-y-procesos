<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button mnemonicParsing="false" onAction="#startSearch" text="Button" />
      <TableView fx:id="amiiboTableView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="SOMETIMES">
        <columns>
            <TableColumn fx:id="amiiboImageColumn" prefWidth="75.0" text="Image" />
            <TableColumn fx:id="amiiboNameColumn" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="amiiboCharacterColumn" prefWidth="75.0" text="Character" />
            <TableColumn fx:id="amiiboSeriesColumn" prefWidth="75.0" text="Amiibo Series" />
            <TableColumn fx:id="amiiboGameSeriesColumn" prefWidth="75.0" text="Game Series" />
            <TableColumn fx:id="amiiboTypeColumn" prefWidth="75.0" text="Type" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
